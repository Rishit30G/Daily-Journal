<%- include('partials/header') %>

<h1 class="newfont">Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label class="newfont">Title</label>
    <input class="form-control" type="text" name="postTitle">
    <label class="newfont">Post</label>
    <textarea class="form-control" name="postBody" rows="4" cols="50"></textarea>
    <button type="submit" name="button" class="btn btn-primary" style="margin-top: 7px">Publish</button>
  </div>

</form>

<%- include('partials/footer'); -%>
